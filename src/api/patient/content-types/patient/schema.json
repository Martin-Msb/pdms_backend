{
  "kind": "collectionType",
  "collectionName": "patients",
  "info": {
    "singularName": "patient",
    "pluralName": "patients",
    "displayName": "patient",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "otherName": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "string",
      "required": true
    },
    "patientPhoto": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "city": {
      "type": "string",
      "required": false
    },
    "address": {
      "type": "string",
      "required": true
    },
    "dob": {
      "type": "date",
      "required": true
    },
    "medicalCondition": {
      "type": "richtext",
      "required": true
    },
    "patient_state": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::patient-state.patient-state"
    },
    "prescription_statuses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::prescription-status.prescription-status"
    },
    "prescriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::prescription.prescription",
      "mappedBy": "patient"
    },
    "patient_visits": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::patient-visit.patient-visit",
      "mappedBy": "patient"
    },
    "diagnoses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::diagnosis.diagnosis",
      "mappedBy": "patient"
    }
  }
}
