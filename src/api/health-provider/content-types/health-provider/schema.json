{
  "kind": "collectionType",
  "collectionName": "health_providers",
  "info": {
    "singularName": "health-provider",
    "pluralName": "health-providers",
    "displayName": "healthProvider",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "licenseNumber": {
      "type": "string",
      "required": true
    },
    "dob": {
      "type": "date",
      "required": true
    },
    "practiceName": {
      "type": "string",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "health_provider"
    },
    "profilePhoto": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "patients": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::patient.patient",
      "mappedBy": "health_providers"
    },
    "institutions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::institution.institution",
      "inversedBy": "health_providers"
    },
    "prescriptions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::prescription.prescription",
      "mappedBy": "health_providers"
    }
  }
}
