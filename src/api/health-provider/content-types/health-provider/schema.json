{
  "kind": "collectionType",
  "collectionName": "health_providers",
  "info": {
    "singularName": "health-provider",
    "pluralName": "health-providers",
    "displayName": "healthProvider",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "licenseNumber": {
      "type": "string",
      "required": true
    },
    "dob": {
      "type": "date",
      "required": true
    },
    "practiceName": {
      "type": "string",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "health_provider"
    },
    "institutions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::institution.institution",
      "mappedBy": "health_provider"
    },
    "profilePhoto": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
